#!/usr/bin/php
<?php
//*
for($i = 30, $k = 40; $i <= 36; $i++, $k++)
	print "\033[01;{$i}mColor$i\033[01;00m\t\t\033[01;{$k}mColors$k\033[00m\n";

print "\033[00m";

